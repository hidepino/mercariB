.mypage
  .mypage_contents
    .mypage__content-box
      .mypage__content-head
        %h3.mypage__content-label 購入した商品
      .mypage__tabs-bottom
        =link_to listing_path,class: "mypage__tabs-list" do
          .tab-active
            .mypage__tabs-text 出品中
        =link_to in_progress_path,class: "mypage__tabs-list" do
          .mypage__tabs-text 取引中
        =link_to completed_path,class: "mypage__tabs-list" do
          .mypage__tabs-text 売却済み
      .mypage__no-item
        .mypage__no-item-icon
        .mypage__no-item-text 出品中の商品はありません
